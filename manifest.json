 {
  "name": "My IP Plugin",
  "description": "Show details of Your IP address and network information.",
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "Arun o v",
 
  "browser_action": { 
    "default_name": "MyIP", 
    "default_title": "Get My IP", 
    "default_icon": "assets/img/icon.png",
    "default_popup": "popup.html"
  }, 
 
  "icons": {
    "120": "assets/img/icon120.png",
    "96" : "assets/img/icon96.png",
    "48" : "assets/img/icon48.png",
    "36" : "assets/img/icon36.png",
    "16" : "assets/img/icon16.png"
  },

  "key": "",
   
  "permissions": [
   "activeTab", 
      "http://www.geoplugin.net/"
   ]   
} 